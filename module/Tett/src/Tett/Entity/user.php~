<?php
namespace Tett\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity(repositoryClass="Tett\Repository\user")
 */
class user
{
	/**
	 * @ORM\Column(type="String")
	 */
	protected $name;

	/**
	 * @ORM\Id
	 * @ORM\GeneratedValue(strategy="AUTO")
	 * @ORM\Column(type="Integer")
	 */
	protected $id;

  
}